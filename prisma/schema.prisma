datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model b2_art {
  id Int @id @default(autoincrement())
  name String @unique
  url String @unique
}

model image {
  id Int @id @default(autoincrement())
  from String @db.VarChar(20)
  url String
  tags String?
  metadata String?
}

model commands {
  id Int @id @default(autoincrement())
  reply String @db.VarChar(255)
  msg String @db.VarChar(255)
}
