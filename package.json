	"version": "1.0.2",
	"description": "Facebook BOT vippro",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"start": "node ./build/main.js",
		"test": "ENV_TAG=development ts-node ./build/main.js",
		"dev": "tsc && node ./build/main.js",
		"prepublish": "npm run build",
		"build": "tsc",
		"generateDocs": "npx typedoc --out docs ./lib/api.ts ./lib/types/incomingMessages.ts ./lib/types/threads.ts ./lib/types/users.ts ./lib/types.ts --excludePrivate --excludeInternal --name \"Facebook Messenger API\" --includeVersion --hideGenerator",
		"migrate": "prisma migrate dev"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Teav4/bot-vippro.git"
	},
	"keywords": [
		"facebook",
		"messenger",
		"api",
		"messaging",
		"message-bot"
	],
	"author": "Filip Makara, Teav4",
	"license": "MIT",
